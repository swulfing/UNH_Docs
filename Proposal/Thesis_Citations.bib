@article{beltonNotJustWealthy2018,
	title = {Not just for the wealthy: {Rethinking} farmed fish consumption in the {Global} {South}},
	volume = {16},
	issn = {22119124},
	shorttitle = {Not just for the wealthy},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S221191241630089X},
	doi = {10.1016/j.gfs.2017.10.005},
	abstract = {Aquaculture’s contributions to food security in the Global South are widely misunderstood. Dominant narratives suggest that aquaculture contributes mainly to international trade beneﬁting richer Northern consumers, or provides for wealthy urban consumers in Southern markets. On the supply side, the literature promotes an idealized vision of ‘small-scale’, low input, semi-subsistence farming as the primary means by which aquaculture can contribute to food security, or emphasizes the role of ‘industrial’ export oriented aquaculture in undermining local food security. In fact, farmed ﬁsh is produced predominantly by a ‘missing middle’ segment of commercial and increasingly intensive farms, and overwhelmingly remains in Southern domestic markets for consumption by poor and middle income consumers in both urban and rural areas, making an important but underappreciated contribution to global food security.},
	language = {en},
	urldate = {2022-12-20},
	journal = {Global Food Security},
	author = {Belton, Ben and Bush, Simon R. and Little, David C.},
	month = mar,
	year = {2018},
	pages = {85--92},
	file = {Belton et al. - 2018 - Not just for the wealthy Rethinking farmed fish c.pdf:C\:\\Users\\sophi\\Zotero\\storage\\7CYV8CP5\\Belton et al. - 2018 - Not just for the wealthy Rethinking farmed fish c.pdf:application/pdf},
}

