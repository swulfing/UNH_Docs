	institution = {In Review},
	author = {Ospina-Alvarez, Andres and Mohan, Silvia de Juan and Pita, Pablo and Ainsworth, Gillian and Matos, Fabio and Pita, Cristina and Villasante, Sebastián},
	month = jul,
	year = {2021},
	doi = {10.21203/rs.3.rs-721404/v1},
	file = {Ospina-Alvarez et al. - 2021 - Main actors in the Cephalopod Global Trade Network.pdf:C\:\\Users\\sophi\\Zotero\\storage\\XBYPNKD7\\Ospina-Alvarez et al. - 2021 - Main actors in the Cephalopod Global Trade Network.pdf:application/pdf},
}

