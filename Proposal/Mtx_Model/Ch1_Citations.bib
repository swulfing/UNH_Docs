@article{baker-medardGenderingMarineConservation2017,
	title = {Gendering {Marine} {Conservation}: {The} {Politics} of {Marine} {Protected} {Areas} and {Fisheries} {Access}},
	volume = {30},
	issn = {0894-1920, 1521-0723},
	shorttitle = {Gendering {Marine} {Conservation}},
	url = {https://www.tandfonline.com/doi/full/10.1080/08941920.2016.1257078},
	doi = {10.1080/08941920.2016.1257078},
	abstract = {Drawing on ethnographic case studies from Madagascar, this research shows that multiple marine conservation projects have institutionalized inequitable access to marine recourses along gendered lines. Despite discursive and institutional shifts toward more “collaborative” and “community-based” conservation programing, there is a deficiency of women’s nominal as well as effective participation in community management organizations. This research shows that conservation organizations’ focus on proximate drivers of marine resource use, or a politics of picking the “low-hanging fruit,” over ultimate drivers such as global commodity chains, places disproportionate emphasis on marine spatial enclosures and restricting specific, and gendered, harvest methods. To address gender bias concerning access to and control over natural resources, we must go beyond the rhetoric of “community involvement” to address gendered inequalities in conservation decision making, and whose interests are served by conservation projects.},
	language = {en},
	number = {6},
	urldate = {2021-11-04},
	journal = {Society \& Natural Resources},
	author = {Baker-Médard, Merrill},
	month = jun,
	year = {2017},
	pages = {723--737},
	annote = {
Differences in fishing methods: Men boat out to fish and women fish on land at low tides-both a difference in catch method AND provides gendered aspect to fishing methods Medard 2017. In Kobalava and Hoalankara, MPA’s restrict women’s access to fishing. In Kobalava this is because the shoreline is now and MPA. In Hoalankara this is because of restrictions. Women if they fish, do so in secret at night. Tsimalaho MPA has a more indirect effect, as the organizations have found land-based livelihoods for fisherwomen (such as basket weaving and handicraft production training), while simultaneously focusing on ways in which to enhance fishermen’s marine-based livelihood (such as fiberglass boat building training to allow fishermen to venture into deeper waters further offshore). Medard 2017
Regendering - a post modern paradox Medard 2017. Two ways:

The selection of conservation project community collaborators, a process that deepened and broadened a form of male leadership that was previously confined to particular contexts
the spatial scaling down of environmental problem identification. This spatial narrowing legitimized conservation efforts focused on local behavior change and spatial enclosures targeting small-scale fisheries, ultimately diverting attention away from very important and more distant drivers of fisheries collapse and ecological decline.


In Madagascar, recent reports show declining catch across multiple fisheries (Le Manach et al. 2012). Although laws exist in Madagascar that ban certain types of fishing gear, restrict certain fisheries seasonally, and prohibit the extraction of endangered marine organisms, the seeming ineffectiveness of these management strategies, along with pressure from the international policy arena to put 10\% of all marine and coastal environments under protection,4 has resulted in a precipitous expansion of MPAs in Madagascar (Cinner et al. 2009; Harris 2011; Corson and MacDonald 2012; Oliver et al. 2015). Shortly after the 2003 World Parks Congress where the former president of Madagascar pledged to dramatically increase the total area under protection in Madagascar (Corson 2011), including 1 million marine hectares (Commission SAPM 2009; Allnutt et al. 2012), the number of MPAs exponentially increased and now both governmental and nongovernmental organizations fund and implement MPAs in every region of the island. Medard 2017
Women fish less frequently than men Medard 2017
In one region surveyed in Madagascar, more than 75\% of all fish caught were sold for export. Focusing on local drivers of resource degradation, such as women’s fishing practice of walking on reefs, thus masks larger economic and political processes that contribute to marine resource degradation Medard 2017
},
	file = {Baker-Médard - 2017 - Gendering Marine Conservation The Politics of Mar.pdf:C\:\\Users\\sophi\\Zotero\\storage\\K7N87Q26\\Baker-Médard - 2017 - Gendering Marine Conservation The Politics of Mar.pdf:application/pdf},
}

@article{benbowLessonsLearntExperimental2014,
	title = {Lessons learnt from experimental temporary octopus fishing closures in south-west {Madagascar}: benefits of concurrent closures},
	volume = {36},
	issn = {1814-232X, 1814-2338},
	shorttitle = {Lessons learnt from experimental temporary octopus fishing closures in south-west {Madagascar}},
	url = {http://www.tandfonline.com/doi/abs/10.2989/1814232X.2014.893256},
	doi = {10.2989/1814232X.2014.893256},
	abstract = {This paper presents evidence of the fisheries effect of experimental temporary fishing closures for Octopus cyanea in the then-emergent Velondriake Locally Managed Marine Area (LMMA) in south-west Madagascar during 2004–2006. We present an analysis of the O. cyanea catch data for the first two years of temporary closures based on landings data collected from village-based octopus collectors. We found a significant closure effect in terms of total weight and number of octopus caught on opening days, but observed that these benefits dissipated quickly, returning to pre-closure levels within days. Mean octopus size increased by 41\% on average when compared to data from selected control sites. However, extremely high levels of fishing effort on opening days meant that these biological effects did not translate into increased weight of octopus caught per successful fisher over the opening tide. Upon opening of concurrent closures during the second round of closures we found significant increases in the weight of octopus caught per successful fisher. We conclude that the pilot closures had a significant closure effect, but caution against isolated openings leading to concentrated fishing effort on opening days.},
	language = {en},
	number = {1},
	urldate = {2021-11-04},
	journal = {African Journal of Marine Science},
	author = {Benbow, S and Humber, F and Oliver, Ta and Oleson, Kll and Raberinary, D and Nadon, M and Ratsimbazafy, H and Harris, A},
	month = jan,
	year = {2014},
	pages = {31--37},
	annote = {Cephs One of the population’s biggest exports since the early 2000s.
Hunting with freedive stick/spear or picked up by hand by women on shore (Benbow 2014 and Mez data). Fishing on land occurs about 7-9 days of the month due to tides. Sailing more constant (Benbow 2014)
Short term MPA’s resulted in rapid bounceback of Octopus (size and number) reemergence due to high spawning rates and short life cycle (Benbow 2014, Katsanevakis 2006, Humber 2006). But also rapid decline once fishing resumed (also result of higher fishing effort in the area following a reopening)
Benbow 2012 has a citation (Raberinary and Benbow 2012) with weight at maturity data for Octopus in Madagascar
 Permanent fishery closures are effective, but can act as a detriment to local fishing efforts (Benbow 2014, Humber 2006)},
	file = {Benbow et al. - 2014 - Lessons learnt from experimental temporary octopus.pdf:C\:\\Users\\sophi\\Zotero\\storage\\VTLF2YDB\\Benbow et al. - 2014 - Lessons learnt from experimental temporary octopus.pdf:application/pdf},
}

