@article{priceEvaluatingRelationshipsWild2014,
	title = {Evaluating {Relationships} between {Wild} {Skeena} {River} {Sockeye} {Salmon} {Productivity} and the {Abundance} of {Spawning} {Channel} {Enhanced} {Sockeye} {Smolts}},
	volume = {9},
	issn = {1932-6203},
	url = {https://dx.plos.org/10.1371/journal.pone.0095718},
	doi = {10.1371/journal.pone.0095718},
	abstract = {The enhancement of salmon populations has long been used to increase the abundance of salmon returning to spawn and/ or to be captured in fisheries. However, in some instances enhancement can have adverse impacts on adjacent nonenhanced populations. In Canada’s Skeena watershed, smolt-to-adult survival of Babine Lake sockeye from 1962–2002 was inversely related to the abundance of sockeye smolts leaving Babine Lake. This relationship has led to the concern that Babine Lake smolt production, which is primarily enhanced by spawning channels, may depress wild Skeena (Babine and non-Babine) sockeye populations as a result of increased competition between wild and enhanced sockeye smolts as they leave their natal lakes and co-migrate to sea. To test this hypothesis we used data on Skeena sockeye populations and oceanographic conditions to statistically examine the relationship between Skeena sockeye productivity (adult salmon produced per spawner) and an index of Babine Lake enhanced smolt abundance while accounting for the potential influence of early marine conditions. While we had relatively high power to detect large effects, we did not find support for the hypothesis that the productivity of wild Skeena sockeye is inversely related to the abundance of enhanced sockeye smolts leaving Babine Lake in a given year. Importantly, life-time productivity of Skeena sockeye is only partially explained by marine survival, and likely is an unreliable measure of the influence of smolt abundance. Limitations to our analyses, which include: (1) the reliance upon adult salmon produced per spawner (rather than per smolt) as an index of marine survival, and (2) incomplete age structure for most of the populations considered, highlight uncertainties that should be addressed if understanding relationships between wild and enhanced sockeye is a priority in the Skeena.},
	language = {en},
	number = {4},
	urldate = {2021-09-14},
	journal = {PLoS ONE},
	author = {Price, Michael H. H. and Connors, Brendan M.},
	editor = {Buckel, Jeffrey},
	month = apr,
	year = {2014},
	keywords = {Salmon, Statistics},
	pages = {e95718},
	annote = {Salmon enhancement may have adverse effects due to higher competition. Statistically test the relationship between Skeena sockeye productivity and enhanced smolt abundance.
Hypothesized issues with enhancement:

overfishing for enhanced salmon
pathogen transfer
increased competition

Tests 8 different hypothesis related to reduced Skeena productivity with 8 different statistical models.
All hypothesis were tested with a modified Ricker stock-recruit relationship model (See: Ricker WE (1975) Computation and interpretation of biological statistics of fish populations.)
Retrospective power analysis: Peterman RM (1990) Statistical power analysis can improve fisheries researchand management
Bootstrap Algorithm: Connors BM, Krkosek M, Ford J, Dill LM (2010). Coho salmon productivity in relation to salmon lice from infected prey and salmon farms
Shortcomings of Model:

Using survival per spawner, not survival per smolt because data doesn't exist
Poor data quality, points out 6 gaps that could lead to further research
},
	file = {Price and Connors - 2014 - Evaluating Relationships between Wild Skeena River.pdf:C\:\\Users\\sophi\\Zotero\\storage\\78VZQF44\\Price and Connors - 2014 - Evaluating Relationships between Wild Skeena River.pdf:application/pdf},
}

@article{thampiSocioecologicalDynamicsCaribbean2018,
	title = {Socio-ecological dynamics of {Caribbean} coral reef ecosystems and conservation opinion propagation},
	volume = {8},
	issn = {2045-2322},
	url = {http://www.nature.com/articles/s41598-018-20341-0},
	doi = {10.1038/s41598-018-20341-0},
	language = {en},
	number = {1},
	urldate = {2021-09-20},
	journal = {Scientific Reports},
	author = {Thampi, Vivek A. and Anand, Madhur and Bauch, Chris T.},
	month = dec,
	year = {2018},
	keywords = {Coral, Social, Modelling},
	pages = {2597},
	annote = {Thampi et al. 2018.  Socio-ecological dynamics of Caribbean coral reef ecosystems and conservation opinion propagation
 
Coral reefs have historically been introduced to various stressors from hurricanes to climate change to food web disturbances. Some basic aspects of this food web are zooxanthellae algae which have a symbiotic relationship with coral, as they live within the coral tissue and receive nutrients while providing organic compounds during the photosynthetic process. The coral also has a mutualistic relationship with sea urchins as they graze on algal turf yet sea urchins are in a state of decline due to various environmental pressures, making way for parrotfish to rise as the primary grazers of algal turf. Macroalgae is known to prevent reef growth as they colonize growth sites for new polyps. This paper aims to model this complex relationship combined with social factors such as fishing intensity. The human aspect is modeled by concepts from game theory, where human opinion or strategies actively change based on the current condition of the environment.
 
To do this, Thampi et al. build on a previous paper’s models (Blackwood et al.) except they add a public opinion component, which influences the population of parrotfish (i.e. fishing intensity and governmental regulation).
 
In their results, they found they could reach an undesirable stable state with a parrotfish growth rate of zero. This results in high microalgal cover (and no coral) as well has high public opinion, which is not sufficient to recover parrotfish levels as the population is not viable. We can get the model to stabilize under a “Macroalgae Free Equilibrium” (MFE), where the parrotfish growth rate outweighs the exploitation rate. It is also possible to model oscillatory behavior over long periods of time, where exploitation increases and declines the coral population which causes a shift in public opinion and reduces fishing intensity.
 
This is the first paper to model coupled socio-ecological dynamics in coral systems. Social pressure can be a key part in promoting conservatism, but only if it is an initially dominant opinion. They also found that the system tended to oscillate between boom and bust cycles, with a few exceptions with specific parameters. Further, under certain parameters, high maximum fishing rates could be counteracted with conservation efforts but these were very specific scenarios.
 
The model does simply the grazer-algal relationship as more species of algae and grazers exist and have more complex life histories and fishing rates, some of these systems have been observed and have higher rates of recovery. They further discuss that there is a lack of data regarding how social norms and sensitivity to coral cover affect attitudes toward conservation. Climate change is also not accounted for in the model.},
	file = {Thampi et al. - 2018 - Socio-ecological dynamics of Caribbean coral reef .pdf:C\:\\Users\\sophi\\Zotero\\storage\\6N3HXQVP\\Thampi et al. - 2018 - Socio-ecological dynamics of Caribbean coral reef .pdf:application/pdf},
}

@article{mooreRestorationEasternOyster2018,
	title = {Restoration of eastern oyster populations with positive density dependence},
	volume = {28},
	issn = {1051-0761, 1939-5582},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/eap.1694},
	doi = {10.1002/eap.1694},
	language = {en},
	number = {4},
	urldate = {2021-09-20},
	journal = {Ecological Applications},
	author = {Moore, Jacob L. and Puckett, Brandon J. and Schreiber, Sebastian J.},
	month = jun,
	year = {2018},
	keywords = {Modelling, Oyster},
	pages = {897--909},
	annote = {Allee effects are defined as positive density dependence where an increase in population size leads to an increase in per capita growth rate. This paper models Allee effects in populations of the Eastern oyster. They provide many benefits to their surrounding environment while simultaneously experiencing population declines due to overfishing, coastal development, disease, and pollution. Positive feedback relationships exist between living oysters and shell substrate. Oyster shells provide solid substrate on which new larvae can attach and grow, thereby creating a reef which provides shelter from predators and preventing burial in sediment. These feedbacks can actually change the Allee threshold for population growth vs. decline and create alternative stable states. Restoration efforts can also alter this threshold and come in the form of stock enhancement, habitat enhancement, and a combination of the two.
 
This paper uses an integral projection model (IPM) to explore three questions: the properties of the Allee threshold and introduce an analytic method for approximating it, the effect of population size structure on the threshold surface, and the relative effectiveness of the two restoration actions. 
 
The resulting model had a hard time assessing growth and survival functions due to data shortages so they used only a size function and then included a maximum age of survival. The proportion of females in the population increased as a function of size. The model found a large region between the two threshold slices where populations beginning at the equilibrium size distribution will increase to infinity, but populations beginning at the harvested size distribution will decline to extinction, even if the population begins above. Additionally, populations beginning at the harvested size distribution will often exhibit complex, oscillatory behavior during the first 10-12 years of the simulation, regardless of whether they are declining toward extinction or increasing toward infinity. Further, adding oysters between the ages of 3-7 were found to have the highest retention rate when it came to conservation efforts (mid-age oysters). Restoration of existing populations depended strongly upon the initial size distribution of the population. One limitation of the model is that it considered these populations “closed” in that there was assumed to be no breeding between sites. The paper argues that more research into the rates of external recruitment could better refine the model.},
	file = {Moore et al. - 2018 - Restoration of eastern oyster populations with pos.pdf:C\:\\Users\\sophi\\Zotero\\storage\\D6KM6HIT\\Moore et al. - 2018 - Restoration of eastern oyster populations with pos.pdf:application/pdf},
}

@article{wassmannFootprintsClimateChange2011,
	title = {Footprints of climate change in the {Arctic} marine ecosystem: {FOOTPRINTS} {OF} {CLIMATE} {CHANGE}},
	volume = {17},
	issn = {13541013},
	shorttitle = {Footprints of climate change in the {Arctic} marine ecosystem},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/j.1365-2486.2010.02311.x},
	doi = {10.1111/j.1365-2486.2010.02311.x},
	abstract = {In this article, we review evidence of how climate change has already resulted in clearly discernable changes in marine Arctic ecosystems. After deﬁning the term ‘footprint’ and evaluating the availability of reliable baseline information we review the published literature to synthesize the footprints of climate change impacts in marine Arctic ecosystems reported as of mid-2009. We found a total of 51 reports of documented changes in Arctic marine biota in response to climate change. Among the responses evaluated were range shifts and changes in abundance, growth/condition, behaviour/phenology and community/regime shifts. Most reports concerned marine mammals, particularly polar bears, and ﬁsh. The number of well-documented changes in planktonic and benthic systems was surprisingly low. Evident losses of endemic species in the Arctic Ocean, and in ice algae production and associated community remained difﬁcult to evaluate due to the lack of quantitative reports of its abundance and distribution. Very few footprints of climate change were reported in the literature from regions such as the wide Siberian shelf and the central Arctic Ocean due to the limited research effort made in these ecosystems. Despite the alarming nature of warming and its strong potential effects in the Arctic Ocean the research effort evaluating the impacts of climate change in this region is rather limited.},
	language = {en},
	number = {2},
	urldate = {2021-09-21},
	journal = {Global Change Biology},
	author = {Wassmann, Paul and Duarte, Carlos M. and Agustí, Susana and Sejr, Mikael K.},
	month = feb,
	year = {2011},
	keywords = {Lit Review, Arctic, Climate Change},
	pages = {1235--1249},
	annote = {Low number of research on planktonic and benthic systems
Limited research in Siberian shelf and Arctic Ocean
Define footprint as "a marked effect or impact"
Climate change affecting arctic 3x more than world average
More freshwater in Arctic
Historical evidence of arctic sea warming spans 5000 years
Lack of consistent research makes it hard to create a "baseline" of normalcy with which to compare
Some Pacific species have been document in the arctic, and shifts in organism ranges has led to food web disruptions
NABOS - Nansen and Amundsen Basins Observational System
Effects on the ecosystem have also been historically attributed to other factors such as fishing/hunting, attributing to poor sampling density
The penetration of warmer Atlantic and Pacific water masses, in increased water residence time, and ice cover have had more effects than direct warming
Ice thought to become seasonal in the next 2-3 decades
Acidification also predicted to occur
Russian-American Long-term Census fo the Arctic (RUSALCA)
Suggest making research efforts in:

Time Series
Adequate seasonal coverage
New technologies
Making older Russian data internationally accessable

Good winter data are only available from the Chukchi Sea (SHEBA), Franklin Bay (CASES) and Banks Island (CFL).
Current knowledge on the Arctic ecosystem can be used strategically to inform new efforts to detect footprints ofclimate change in the Arctic where they are criticallyneeded. We need first of all basic knowledge from threekey regions: (a) the Fram Strait area that accounts formore than 50\% of all exchanges of water to and from the central Arctic Ocean (Schauer et al., 2004); (b) theextensive Siberian shelf, to which none of the footprintsidentified here refer, but which is a key area to detectchanges because of the very large increase in riverdischarge of water and materials (e.g. sediments, dissolved organic matter, nutrients, toxic substances) withclimate warming (Peterson et al., 2002; Fortier, 2008);and (c) the Central Arctic Ocean, arguably the leaststudied region of the ocean, for which no ecologicalinformation on biological impacts of climate change isavailable.
Remote sensing not as effective here due to cloud and ice cover
 },
	file = {Wassmann et al. - 2011 - Footprints of climate change in the Arctic marine .pdf:C\:\\Users\\sophi\\Zotero\\storage\\6U9GBUM4\\Wassmann et al. - 2011 - Footprints of climate change in the Arctic marine .pdf:application/pdf},
}

@article{sigdelCompetitionInjunctiveSocial2017,
	title = {Competition between injunctive social norms and conservation priorities gives rise to complex dynamics in a model of forest growth and opinion dynamics},
	volume = {432},
	issn = {00225193},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0022519317303600},
	doi = {10.1016/j.jtbi.2017.07.029},
	abstract = {Human systems and environmental systems are often treated as existing in isolation from one another, whereas in fact they are often two parts of a single, coupled human-environment system. Developing theoretical models of coupled human-environment systems is a continuing area of research, although relatively few of these models are based on diﬀerential equations. Here we develop a simple diﬀerential equation coupled human-environment system model of coupled forest growth dynamics and conservationist opinion dynamics in a human population. The model assumes logistic growth and harvesting in the forest. Opinion spread in the human population is based the interplay between conservation values stimulated by forest rarity, and injunctive social norms that tend to support population conformity. We ﬁnd that injunctive social norms drive the system to the boundaries of phase space, whereas rarity-based conservation priorities drive the system to the interior. The result is complex dynamics including limit cycles and alternative stable states that do not occur if injunctive social norms are absent. We also ﬁnd that increasing the inherent perceived value of forests is the best way to boost and stabilize forest cover while also boosting conservationist opinion in the population. We conclude that simple models can provide insights and suggest patterns that might be harder to see with complicated, high-dimensional models, and therefore should be pursued more often in research on coupled human-environment systems.},
	language = {en},
	urldate = {2021-09-27},
	journal = {Journal of Theoretical Biology},
	author = {Sigdel, Ram P. and Anand, Madhur and Bauch, Chris T.},
	month = nov,
	year = {2017},
	keywords = {Social, Modelling, Forest},
	pages = {132--140},
	annote = {Relatively few social interaction models are based on differential equations - What else are they based off of?
Model assumes logistic growth
Markov chain models study the effect of social learning on management decisions.
What is an agent based model?
Past models have looked at:

